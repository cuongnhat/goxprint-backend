{
    "name": "goxprint-driver-manager",
    "version": "1.0.0",
    "description": "GoXPrint Driver Manager - Admin panel with API for managing printer drivers",
    "type": "module",
    "scripts": {
        "dev": "concurrently \"npm run dev:api\" \"npm run dev:web\"",
        "dev:web": "vite --port 3000",
