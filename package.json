    "dependencies": {
        "@aws-sdk/client-s3": "^3.478.0",
        "@aws-sdk/s3-request-presigner": "^3.478.0",
        "adm-zip": "^0.5.16",
        "better-sqlite3": "^12.5.0",
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "multer": "^1.4.5-lts.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.20.0",
        "ssh2": "^1.17.0",
        "ws": "^8.18.3"
    },
    "devDependencies": {
        "@types/react": "^18.2.43",
        "@types/react-dom": "^18.2.17",
        "@vitejs/plugin-react": "^4.2.1",
        "concurrently": "^8.2.2",
        "typescript": "^5.3.3",
        "vite": "^5.0.8"
    }
